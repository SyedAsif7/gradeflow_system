{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "includeFiles": ["**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo-url",
    "DB_NAME": "@db-name",
    "JWT_SECRET": "@jwt-secret",
    "CORS_ORIGINS": "@cors-origins"
  }
}