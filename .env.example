# GradeFlow System - Docker Compose Environment Variables
# Copy this to .env and configure for your environment

# MongoDB Configuration
MONGO_ROOT_USER=admin
MONGO_ROOT_PASSWORD=your_secure_password_here
MONGO_URL=mongodb://admin:your_secure_password_here@mongo:27017/gradeflow_db?authSource=admin
DB_NAME=gradeflow_db

# Backend Configuration
ENVIRONMENT=development
LOG_LEVEL=INFO
JWT_SECRET=your-secret-key-change-this-in-production-minimum-32-characters

# Frontend Configuration
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_API_TIMEOUT=30000
